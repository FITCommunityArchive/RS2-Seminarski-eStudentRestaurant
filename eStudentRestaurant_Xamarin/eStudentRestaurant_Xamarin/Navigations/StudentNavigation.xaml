<?xml version="1.0" encoding="utf-8" ?>
<MasterDetailPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="eStudentRestaurant_Xamarin.Navigations.StudentNavigation"
             xmlns:nav="clr-namespace:eStudentRestaurant_Xamarin.Navigations"
             xmlns:pages="clr-namespace:eStudentRestaurant_Xamarin" 
             MasterBehavior="Popover">
  <MasterDetailPage.Master>
    <nav:StudentNavigationMaster x:Name="MasterPage" />
  </MasterDetailPage.Master>
  <MasterDetailPage.Detail>
    <NavigationPage>
      <x:Arguments>
        <pages:StudentMainPage />
      </x:Arguments>
    </NavigationPage>
  </MasterDetailPage.Detail>
</MasterDetailPage>